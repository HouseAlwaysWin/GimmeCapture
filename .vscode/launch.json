{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug GimmeCapture",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "dotnet",
            "args": [
                "run",
                "--project",
                "${workspaceFolder}/src/GimmeCapture/GimmeCapture.csproj",
                "--no-build"
            ],
            "cwd": "${workspaceFolder}/src/GimmeCapture",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}